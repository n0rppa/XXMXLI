Header set Content-Security-Policy "default-src 'self' cdn.plyr.io cdnjs.cloudflare.com unpkg.com; script-src 'self' 'unsafe-inline' cdn.plyr.io cdnjs.cloudflare.com unpkg.com; style-src 'self' 'unsafe-inline' cdn.plyr.io cdnjs.cloudflare.com; media-src 'self' blob: *; connect-src 'self' *; font-src 'self' cdn.jsdelivr.net cdnjs.cloudflare.com"
Header unset Speculation-Rules
Header unset Origin-Trial
Header always set X-Content-Type-Options "nosniff"
Header always set X-Frame-Options "DENY"
Header always set Referrer-Policy "strict-origin-when-cross-origin"
AddType audio/flac .flac
AddType audio/mpeg .mp3
